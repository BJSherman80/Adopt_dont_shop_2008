<div class="pageheader">
  <h1><%= @shelter.name %></h1>
</div>
<% @shelter_pets.each do |pet| %>
  <div class="column">
    <h4>Name: <a href="/pets/<%= pet.id %>"><%= pet.name %></a></h4>
    <p>Age: <%= pet.age%></p>
    <p>Sex: <%= pet.sex%></p>
    <p>Image: <%=image_tag "#{pet.image}"%></p>
  </div>
<% end %>
</div>
